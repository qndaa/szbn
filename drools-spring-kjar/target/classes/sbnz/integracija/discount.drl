package sbnz.integracija;

import com.example.enums.CategoryOfUser;
import com.example.model.*;


rule "Silver Users - 10%"
    when
        $s: Subscriber(categoryOfUser == CategoryOfUser.SILVER)
    then
        System.out.println("Usao u pravilo SILVER");
        $s.setDiscount(10.0);
        update($s);
end
